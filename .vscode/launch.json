{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C#: AppHost [Default Configuration]",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/AppHost/bin/Debug/net9.0/AppHost.dll",
      "args": [],
      "cwd": "${workspaceFolder}/AppHost",
      "console": "internalConsole",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "C#: ApiService [Default Configuration]",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ApiService/bin/Debug/net9.0/ApiService.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ApiService",
      "console": "internalConsole",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}